<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.App_Login" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Darker_Teal</item>
        <item name="colorPrimaryDark">@color/Teal</item>
        <item name="colorAccent">@color/Beige</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.App_Login.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.App_Login.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.App_Login.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>